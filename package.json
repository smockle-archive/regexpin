{
  "name": "regexpin",
  "version": "1.0.2",
  "description": "Like Immutable.JS' Iterable#getIn, but with RegExp support.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npm run test:bash",
    "test:bash": "tape \"tests/**/*.js\" && snyk auth $SNYK_TOKEN && snyk test",
    "test:cmd": "tape \"tests/**/*.js\" && snyk auth %SNYK_TOKEN% && snyk test",
    "coverage": "istanbul cover tape \"tests/**/*.js\"",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "postpublish": "greenkeeper-postpublish"
  },
  "betterScripts": {
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/regexpin.git"
  },
  "author": "Clay Miller <clay@smockle.com> (http://www.smockle.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smockle/regexpin/issues"
  },
  "homepage": "https://github.com/smockle/regexpin#readme",
  "dependencies": {
    "immutable": "^3.7.6",
    "snyk": "^1.24.4"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.4.0",
    "coveralls": "^2.11.6",
    "eslint": "^3.0.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "greenkeeper-postpublish": "^1.0.0",
    "istanbul": "^0.4.2",
    "tape": "^4.4.0"
  },
  "snyk": true
}
